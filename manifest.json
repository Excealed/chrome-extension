{
    "manifest_version": 3,
    "name": "Super Private VPN",
    "version": "1.0",
    "description": "A brief description of your extension",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["script.js"]
      }
    ]
}